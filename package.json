{
  "name": "@ai-integration/course",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://ai-integra-course-v2.web.app",
  "dependencies": {
    "@capacitor/core": "^6.2.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "ajv": "^8.17.1",
    "animejs": "^4.1.4",
    "axios": "^1.12.2",
    "develop": "^1.0.0",
    "devlop": "^1.1.0",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "express": "^5.1.0",
    "firebase": "^11.10.0",
    "googleapis": "^162.0.0",
    "micromark-core-commonmark": "^2.0.3",
    "next": "^16.0.10",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-player": "^2.16.1",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.0.1",
    "react-simple-code-editor": "^0.14.1",
    "remark": "^15.0.1",
    "remark-gfm": "^2.0.0",
    "stripe": "^18.5.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "preinstall": "echo 'Installing dependencies...'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "node scripts/run-build.js",
    "build:production": "NODE_ENV=production npm run build",
    "test": "vitest run",
    "eject": "react-scripts eject",
    "clean": "rm -rf build node_modules functions/node_modules functions/lib .firebase",
    "install:all": "npm install && cd functions && npm install && cd ..",
    "setup:python": "python3 -m venv venv && ./venv/bin/pip install -r python-requirements.txt",
    "prepare:hosting": "node scripts/prepare-hosting.cjs",
    "deploy": "npm run install:all && npm run build:production && firebase deploy --only hosting --project ai-integra-course-v2",
    "deploy:hosting": "npm run build:production && firebase deploy --only hosting --project ai-integra-course-v2",
    "deploy:functions": "cd functions && npm install && npm run build && cd .. && firebase deploy --only functions --project ai-integra-course-v2",
    "deploy:prod": "npm run clean && npm run install:all && npm run setup:python && npm run build:production && firebase deploy --project ai-integra-course-v2",
    "deploy:gcloud": "bash scripts/gcloud-deploy.sh",
    "gcloud:setup": "bash scripts/gcloud-setup.sh",
    "gcloud:validate": "bash scripts/gcloud-validate.sh",
    "preview": "npm run build && npx serve -s build",
    "artifactregistry-login": "npx google-artifactregistry-auth --repo-config=./.npmrc --credential-config=./.npmrc",
    "setup": "bash setup.sh",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.1",
    "@types/animejs": "^3.1.13",
    "@types/express": "^5.0.3",
    "@types/node-fetch": "^2.6.13",
    "@types/prismjs": "^1.26.5",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^6.5.0",
    "firebase-tools": "^13.35.1",
    "google-artifactregistry-auth": "^3.5.0",
    "jsdom": "^27.0.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.5.6",
    "react-compiler-webpack": "^0.2.1",
    "tailwindcss": "^3.4.17",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0",
    "npm": ">=11.0.0"
  },
  "description": "A comprehensive platform for learning about AI integration and investment strategies in the age of artificial intelligence.",
  "main": "postcss.config.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MicroAIStudios-DAO/ai-integration-course-v2.git"
  },
  "keywords": [],
  "author": "",
