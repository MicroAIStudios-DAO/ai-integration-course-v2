        "contactType": "customer support"
      }
    }
    </script>
    
    <!-- Structured Data - Course -->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "Course",
      "name": "AI Integration Course",
      "description": "Comprehensive course on AI integration and investment strategies. Learn to outthink the AI hype and invest with confidence.",
      "provider": {
        "@type": "Organization",
        "name": "Golden Age Mindset",
        "sameAs": "https://aiintegrationcourse.com"
      },
      "offers": {
        "@type": "Offer",
        "category": "Subscription",
        "price": "0",
        "priceCurrency": "USD",
        "availability": "https://schema.org/InStock",
        "validFrom": "2024-01-01",
        "description": "7-day free trial, then premium subscription"
      },
      "educationalLevel": "Beginner to Advanced",
